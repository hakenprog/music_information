<p id="notice"><%= notice %></p>

<h1>Artist Albums</h1>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>Artist</th>
      <th>Album</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artist_albums.each do |artist_album| %>
      <tr>
        <td><%= artist_album.artist_id %></td>
        <td><%= artist_album.album_id %></td>
        <td><%= link_to 'Show', artist_album %></td>
        <td><%= link_to 'Edit', edit_artist_album_path(artist_album) %></td>
        <td><%= link_to 'Destroy', artist_album, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

